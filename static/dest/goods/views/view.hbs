<div id="logined"></div>
</div>
</div>
<footer>Footer</footer>
<script order="0">
var linkUrl={user:'{{constant.linkUrl.user}}'},rendered={{variable.rendered}};
var dataUrl={render:'{{{constant.dataUrl.render}}}'};
</script>
<script class="jstpl" tpl="logined" type="text/x-handlebars-template">
logined: \{{#if logined}}\{{logined}}\{{else}}0\{{/if}}
</script>
<script order="1" src="{{constant.URL.RESOURCE}}/bower_components/jquery-legacy/dist/jquery.min.js?{{hash.bower.jquery-legacy}}"></script>

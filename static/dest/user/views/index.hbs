<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>{{meta.title}}</title>
<meta name="keywords" content="{{meta.keywords}}" />
<meta name="description" content="{{meta.description}}" />
<link rel="stylesheet" href="{{constant.URL.RESOURCE}}/common/res/css/base.css?{{hash.css.common_res_css_base}}" />
<link rel="stylesheet" href="{{constant.URL.RESOURCE}}/common/res/css/sprite.css?{{hash.css.common_res_css_sprite}}" />
<link rel="stylesheet" href="{{constant.URL.RESOURCE}}/common/libs/header/style.css?{{hash.css.common_libs_header_style}}" />
<link rel="stylesheet" type="text/css" href="{{constant.URL.RESOURCE}}/common/libs/footer/style.css?{{hash.css.common_libs_footer_style}}" />
</head>
<body>
<header>Header</header>
<div class="ar">
  <div class="arc">
<div class="bx0">
  <h1>会员信息</h1>
  <ul class="jstpl" tpl="user_info" id="user_info">
    <li>会员名：\{{user.username}}</li>
    <li>金额：\{{user.money}}</li>
  </ul>
</div>
<div class="ico ico-black_50"></div>
<img class="lazy" src="{{constant.URL.RESOURCE}}/common/res/img/tp.png?{{hash.img.common_res_img_tp_png}}" data="{{constant.URL.RESOURCE}}/common/res/img/red_100.jpg?{{hash.img.common_res_img_red_100_jpg}}" alt="red_100" />
</div>
</div>
<footer>Footer</footer>
<script order="0">
var linkUrl={user:'{{constant.linkUrl.user}}'},rendered={{variable.rendered}};
var dataUrl={render:'{{{constant.dataUrl.render}}}'};
</script>
<script>
var dataUrl={render:'{{{constant.dataUrl.render}}}'};
</script>
<script order="1" src="{{constant.URL.RESOURCE}}/bower_components/jquery-legacy/dist/jquery.min.js?{{hash.bower.jquery-legacy}}"></script>
<script order="2" src="{{constant.URL.RESOURCE}}/bower_components/js-cookie/src/js.cookie.min.js?{{hash.bower.js-cookie}}"></script>
<script order="3" src="{{constant.URL.RESOURCE}}/bower_components/handlebars/handlebars.min.js?{{hash.bower.handlebars}}"></script>
<script order="4" src="{{constant.URL.RESOURCE}}/common/res/js/base.js?{{hash.js.common_res_js_base}}"></script>
</body>
</html>
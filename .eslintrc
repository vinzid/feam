{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["error", 2, {"SwitchCase": 1}],
    "no-undef": 0,
    "no-unused-vars": 0,
    "camelcase": 2,
    "no-redeclare": 0,
    "no-console": 0,
    "no-empty": ["error", {"allowEmptyCatch": true}]
  }
}